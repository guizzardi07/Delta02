library(readxl)
Relevamientos <- read.csv('Relevamiento.csv')
summary(Relevamientos)
Sumario = summary(Relevamientos)
capture.output(Sumario, file="Sumario.doc")
boxplot(Relevamientos$Dif_MDE_Ra, horizontal = TRUE, col = 'lightblue', boxwex = 0.5, xlab = 'Diferencias [m]')
hist(Relevamientos$Dif_MDE_Ra, ylim = c(0, 0.8), xlim=c(-5,5), prob = T, main = NULL, xlab = 'Diferencias MDE Rafael [m]', ylab = 'Densidad', breaks = 30, col = '1024')
lines(density(Relevamientos$Dif_MDE_Ra))
library(ggplot2)
ggplot(Relevamientos, aes(x = Relevamientos$Dif_MDE_Ra, fill = Relevamientos$layer)) + geom_density(position = "identity", col = NA, alpha = 0.3) + labs(x="Diferencias [m]", y = "Densidad")
ggplot(Relevamientos, aes(x = Relevamientos$Dif_MDE_Ra, fill = Relevamientos$layer)) + geom_density(position = "identity", col = NA, alpha = 0.3) + labs(fill = 'Relevamiento', x="Diferencias [m]", y = "Densidad")
